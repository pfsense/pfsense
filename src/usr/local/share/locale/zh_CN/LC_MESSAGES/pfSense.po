#: src/usr/local/www/system_gateways_edit.php:348
msgid "Alert interval"
msgstr "警报间隔"

#: src/usr/local/www/system_gateways_edit.php:358
#, php-format
msgid ""
"Time interval in milliseconds between checking for an alert condition. "
