"&quot;zone&quot; type=&quot;hidden&quot; value="
"&quot;$PORTAL_ZONE$&quot;&gt;%1$s&nbsp;&nbsp;&nbsp;&lt;input name="
"&quot;accept&quot; type=&quot;submit&quot; value="
"&quot;Continue&quot;&gt;%1$s&lt;/form&gt;"

