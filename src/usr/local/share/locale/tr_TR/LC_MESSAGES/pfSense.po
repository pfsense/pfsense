#: src/usr/local/www/interfaces_bridge_edit.php:421
msgid "*Member Interfaces"
msgstr "* Üye Arayüzleri"

#: src/usr/local/www/interfaces_bridge_edit.php:425
msgid "Interfaces participating in the bridge."
msgstr "Köprüye katılan arayüzler."

#: src/usr/local/www/interfaces_bridge_edit.php:445
#: src/usr/local/www/firewall_rules_edit.php:1542
