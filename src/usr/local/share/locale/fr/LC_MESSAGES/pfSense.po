"&quot;password&quot;&gt;%1$s&nbsp;&nbsp;&nbsp;&lt;input name="
"&quot;auth_voucher&quot; type="
"&quot;text&quot;&gt;%1$s&nbsp;&nbsp;&nbsp;&lt;input name="
"&quot;redirurl&quot; type=&quot;hidden&quot; value="
