msgstr "修订日期/时间"

msgid "Revision Reason"
msgstr "备份原因"

msgid "SHA256 summary"
msgstr "SHA256摘要"

msgid "Encrypted config.xml"
msgstr "加密的config.xml"

msgid "Decrypted config.xml"
msgstr "解密的config.xml"

